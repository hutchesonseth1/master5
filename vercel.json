{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "cd apps/quick-legal-biz && pnpm build",
  "builds": [
    {
      "src": "apps/quick-legal-biz/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/quick-legal-biz/$1"
    }
  ]
}